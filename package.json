{
  "name": "@vestfoldfylke/azure-blob-client",
  "version": "0.1.7",
  "description": "A client for working with Azure storage blobs",
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.cjs",
      "import": "./dist/esm/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "package.json"
  ],
  "engines": {
    "node": ">=22"
  },
  "scripts": {
    "build": "npm run test && tsc && rollup -c",
    "dev": "npm run build && node --env-file=.env.development ./dist/cjs/tests/dev.cjs",
    "lint:fix": "biome check --write",
    "test": "biome check"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vestfoldfylke/azure-blob-client.git"
  },
  "keywords": [
    "Azure",
    "blob",
    "blobs"
  ],
  "author": {
    "name": "Rune Moskvil Lyng√•s",
    "email": "rune.moskvil.lyngas@vestfoldfylke.no",
    "url": "https://github.com/runely"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/vestfoldfylke/azure-blob-client/issues"
  },
  "homepage": "https://github.com/vestfoldfylke/azure-blob-client#readme",
  "dependencies": {
    "@azure/storage-blob": "^12.30.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.11",
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@types/node": "^25.0.9",
    "rollup-plugin-dts": "^6.3.0",
    "rollup-plugin-esbuild": "^6.2.1"
  }
}
