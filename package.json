{
  "name": "@vestfoldfylke/azure-blob-client",
  "version": "1.0.0",
  "description": "A client for working with Azure storage blobs",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "files": [
    "dist",
    "package.json"
  ],
  "engines": {
    "node": ">=22"
  },
  "scripts": {
    "build": "npm run test && tsc",
    "dev": "npm run build && node --env-file=.env.development ./build/tests/dev.js",
    "lint:fix": "biome check --write",
    "pack": "npm run test && rm -rf dist && rm -f vestfoldfylke-azure-blob-client-*.tgz && npm run build && npm pack",
    "test": "biome check && tsc -p tsconfig_tests.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vestfoldfylke/azure-blob-client.git"
  },
  "keywords": [
    "Azure",
    "blob",
    "blobs"
  ],
  "author": {
    "name": "Rune Moskvil Lyng√•s",
    "email": "rune.moskvil.lyngas@vestfoldfylke.no",
    "url": "https://github.com/runely"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/vestfoldfylke/azure-blob-client/issues"
  },
  "homepage": "https://github.com/vestfoldfylke/azure-blob-client#readme",
  "dependencies": {
    "@azure/storage-blob": "^12.30.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.13",
    "@types/node": "^25.0.10",
    "typescript": "^5.9.3"
  }
}
