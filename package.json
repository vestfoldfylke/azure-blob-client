{
  "name": "@vtfk/azure-blob-client",
  "version": "0.1.0",
  "description": "A client for working with Azure storage blobs",
  "main": "index.js",
  "scripts": {
    "clear_types": "del-cli \"**/*.d.ts\" \"!node_modules\"",
    "tsc": "npm run clear_types && tsc",
    "dev": "cross-env NODE_ENV=development nodemon ./tests/dev.js",
    "test": "standardx --fix && npm run tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vtfk/azure-blob-client.git"
  },
  "keywords": [
    "Azure",
    "blob",
    "blobs"
  ],
  "author": "Karl-Einar Bergstr√∏m [VTFK]",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/vtfk/azure-blob-client/issues"
  },
  "homepage": "https://github.com/vtfk/azure-blob-client#readme",
  "dependencies": {
    "@azure/storage-blob": "^12.8.0",
    "dotenv": "^10.0.0"
  },
  "devDependencies": {
    "@types/jest": "27.0.3",
    "cross-env": "7.0.3",
    "del-cli": "4.0.1",
    "jest": "27.4.7",
    "nodemon": "^2.0.21",
    "standardx": "7.0.0",
    "tsc": "2.0.4",
    "typescript": "4.5.5"
  },
  "standardx": {
    "env": [
      "jest"
    ],
    "ignore": [
      "tests/dev.js"
    ]
  }
}
